{
    "title" : "Leet学习笔记",
    "author" : "黄文庆",
    "description": "",
    "language" : "zh-hans",
    "plugins" : [ 
      "code" ,
      "theme-comscore",
      "search-plus",
      "expandable-chapters",
      "splitter" ,
      "popup",
      "katex",
      "emphasize",
      "github-buttons",
      "prism",
      "-highlight"
    ],



    "pluginsConfig": {
      "theme-default": {
        "treeLevel": true
      },
      "code": {
          "copyButtons": true
      },
      "github-buttons": {
        "buttons": [{
          "user": "micro-coder",
          "repo": "Leet-Note",
          "type": "star",
          "size": "small"
        }, {
          "user": "micro-coder",
          "type": "follow",
          "width": "230",
          "count": true
        }]
      },
      "prism": {
        "css": [
            "prismjs/themes/prism-tomorrow.css"
        ]
      },
      "mathjax":{
         "forceSVG": true
      }
    }
}